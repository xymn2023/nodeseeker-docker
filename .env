# Server Configuration
PORT=3010
HOST=0.0.0.0
NODE_ENV=development
CORS_ORIGINS=http://localhost:3010             

# Database Configuration
DATABASE_PATH=./data/nodeseeker.db

# JWT 密钥
JWT_SECRET=必填内容


